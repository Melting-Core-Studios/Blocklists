{
  "name": "[ MCS ] Begone Annoyances",
  "description": "A blocklist that will remove all annoyances from online browsing.",
  "homepage": "https://github.com/Melting-Core-Studios/Blocklists",
  "license": "CC BY-SA 4.0",
  "version": "1.3",
  "sources": [
    {
      "source": "https://filters.adtidy.org/android/filters/14_optimized.txt",
      "type": "adblock"
    },
    {
      "source": "https://filters.adtidy.org/android/filters/21_optimized.txt",
      "type": "adblock"
    },
    {
      "source": "https://filters.adtidy.org/android/filters/19_optimized.txt",
      "type": "adblock"
    },
    {
      "source": "https://filters.adtidy.org/android/filters/18_optimized.txt",
      "type": "adblock"
    },
    {
      "source": "https://filters.adtidy.org/android/filters/20_optimized.txt",
      "type": "adblock"
    },
    {
      "source": "https://easylist-downloads.adblockplus.org/antiadblockfilters.txt",
      "type": "adblock"
    },
    {
      "source": "https://secure.fanboy.co.nz/fanboy-annoyance_ubo.txt",
      "type": "adblock"
    },
    {
      "source": "https://filters.adtidy.org/android/filters/22_optimized.txt",
      "type": "adblock"
    }
  ],
  "transformations": [
    "RemoveComments",
    "Deduplicate",
    "RemoveEmptyLines",
    "TrimLines",
    "InsertFinalNewLine"
  ],
  "exclusions": [
    "@@||accounts.google.com^$important",
    "@@||login.microsoftonline.com^$important"
  ]
}
